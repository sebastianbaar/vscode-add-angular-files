{
  "name": "vscode-add-angular-files",
  "displayName": "Add Angular Files",
  "description": "Add Angular files including snippets to your Visual Studio Code project",
  "version": "2.0.0",
  "icon": "images/icon.png",
  "publisher": "sebastianbaar",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "Sebastian Baar"
  },
  "keywords": [
    "Angular",
    "Angular 2",
    "Angular 4",
    "TypeScript",
    "Add files"
  ],
  "engines": {
    "vscode": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastianbaar/vscode-add-angular-files"
  },
  "bugs": {
    "url": "https://github.com/sebastianbaar/vscode-add-angular-files/issues"
  },
  "categories": [
    "Other",
    "Languages",
    "Snippets"
  ],
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.addAngularFiles",
        "title": "Add Angular Files"
      },
      {
        "command": "extension.addAngularFilesExtended",
        "title": "Add Angular Files (Extended)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "",
          "command": "extension.addAngularFiles",
          "group": "Add Files"
        },
        {
          "when": "",
          "command": "extension.addAngularFilesExtended",
          "group": "Add Files"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Add Angular Files configuration",
      "properties": {
        "addAngular.stylesheet": {
          "type": "string",
          "default": "css",
          "description": "Specifies stylesheet language ('css', 'sass', 'less').",
          "enum": [
            "css",
            "sass",
            "less"
          ]
        },
        "addAngular.addTestFile": {
          "type": "boolean",
          "default": true,
          "description": "Create a test file (.spec)"
        }
      }
    }
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.1.0",
    "rimraf": "2.6.1",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32",
    "@types/q": "^1.0.0",
    "@types/rimraf": "0.0.28"
  },
  "dependencies": {
    "q": "^1.4.1",
    "path": "^0.12.7"
  }
}